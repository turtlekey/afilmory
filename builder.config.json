{
  "repo": {
    "enable": true,
    "url": "https://github.com/turtlekey/afilmory-assets"
  },
  "storage": {
    "provider": "s3",
    "bucket": "picimpact",
    "region": "auto",
    "prefix": "",
    "customDomain": "",
    "endpoint": "https://oss-cn-hangzhou.aliyuncs.com"
  },
  "options": {
    "defaultConcurrency": 8,
    "enableLivePhotoDetection": true,
    "showProgress": true,
    "showDetailedStats": true
  },
  "logging": {
    "verbose": true,
    "level": "info",
    "outputToFile": false
  },
  "performance": {
    "worker": {
      "workerCount": 8,
      "timeout": 30000,
      "useClusterMode": true,
      "workerConcurrency": 2
    }
  }
}
